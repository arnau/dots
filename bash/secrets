RO_GITHUB_KEY=$(pass github/tokens/ro)
GITHUB_KEY_ALCHEMIST=$(pass github/tokens/alchemist)
CLOUDFLARE_TOKEN=$(pass cloudflare/token)

export AWS_CONFIG_FILE="$HOME/.aws/config"

export RO_GITHUB_KEY
export GITHUB_KEY_ALCHEMIST
export GITHUB_INSIGHTS_TOKEN=$GITHUB_KEY_ALCHEMIST
export GITHUB_TOKEN=$GITHUB_KEY_ALCHEMIST

export CLOUDFLARE_TOKEN
export SECRETS_READY=1
